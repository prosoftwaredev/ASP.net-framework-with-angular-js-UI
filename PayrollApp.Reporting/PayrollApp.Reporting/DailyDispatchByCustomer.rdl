  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText> SELECT "RsvActivities"."Start", "Customers"."CustomerName", "RsvActivities"."ActivityNum", "Personnel"."LastName", "Personnel"."FirstName", "RsvActivities"."BillState", 
 "Reservations"."Notes", "RsvActivities"."Details"
-- ,dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))
 ,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/Place/node())[1]', 'nvarchar(max)')
ELSE '-'
END
END
 as Location
 ,  Isnull(Personnel.LastName,' ') +  ' '+ Isnull(Personnel.FirstName,' ') As EmployeeName
 ,  Case When RsvActivities.BillState=-2 Then 'OPEN'
			When  RsvActivities.BillState=-1 Then 'OK'
			Else 'COMP'
			END AS 'Status'
,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
		Case When len(Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)'))&gt;0
		Then Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)')
		Else Cast(Replace(Replace("Reservations"."Notes",'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)')
		End
ELSE '-'
END
END
 as ReportTo
 ,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/DispatchNote/node())[1]', 'nvarchar(max)')
ELSE '-'
END
END
 as 'Comment'
 ,Left(convert(varchar, Start, 8) ,2) as 'Starting'
 FROM   (("dbo"."Reservations" "Reservations" INNER JOIN "dbo"."RsvActivities" "RsvActivities" ON "Reservations"."ID_Rsv"="RsvActivities"."ID_Rsv") LEFT OUTER JOIN "dbo"."Personnel" "Personnel" ON "RsvActivities"."ID_Personnel"="Personnel"."ID_Personnel") INNER JOIN "dbo"."Customers" "Customers" ON "RsvActivities"."ID_Customer"="Customers"."ID_Customer"
 WHERE  "RsvActivities"."BillState"&lt;0
 And Details   like '%&lt;place&gt;%'
And Convert(Varchar,Start,106)='03 FEB 2017'
 Order by Left(convert(varchar, Start, 8) ,2)
    </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT "RsvActivities"."Start", "Customers"."CustomerName", "RsvActivities"."ActivityNum",
  "Personnel"."LastName"+' '+ "Personnel"."FirstName" As EmpName,
  Customer_Phone=(
 Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
		Case When len(Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/Phone/node())[1]', 'nvarchar(max)'))&gt;0
		Then Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/Phone/node())[1]', 'nvarchar(max)')
		 when  len(Cast(Replace(Replace("Reservations"."Notes",'`',' '),'&amp;','-') as XML).value('(/Phone/node())[1]', 'nvarchar(max)')) &gt; 0
		Then Cast(Replace(Replace("Reservations"."Notes",'`',' '),'&amp;','-') as XML).value('(/Phone/node())[1]', 'nvarchar(max)')
		 When len(Customers.Phone)&gt;0
		Then Customers.Phone
		Else 
				Customers.BillToPhone
		End
	 End
END
)
  ,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/Place/node())[1]', 'nvarchar(max)')
ELSE '-'
END
END
 as Location,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
		Case When len(Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)'))&gt;0
		Then Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)')
		Else Cast(Replace(Replace("Reservations"."Notes",'`',' '),'&amp;','-') as XML).value('(/ReportTo/node())[1]', 'nvarchar(max)')
		End
ELSE '-'
END
END
 as ReportTo,
 PlaceBy=(
 Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
		Case When len(Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/PlacedBy/node())[1]', 'nvarchar(max)'))&gt;0
		Then Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/PlacedBy/node())[1]', 'nvarchar(max)')
		Else Cast(Replace(Replace("Reservations"."Notes",'`',' '),'&amp;','-') as XML).value('(/PlacedBy/node())[1]', 'nvarchar(max)')
		End
ELSE '-'
END
END
 
 )
 ,Case when   ("RsvActivities".Details) is   NULL Then '-' else       
  CASE WHEN dbo.isValidXML(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-'))=0  THEN 
Cast(Replace(Replace("RsvActivities".Details,'`',' '),'&amp;','-') as XML).value('(/DispatchNote/node())[1]', 'nvarchar(max)')
ELSE '-'
END
END
 as 'Comment'
 , "RsvActivities"."BillState", "Reservations"."Notes", "RsvActivities"."Details"
 FROM   (("dbo"."Reservations" "Reservations" INNER JOIN "dbo"."RsvActivities" "RsvActivities" ON "Reservations"."ID_Rsv"="RsvActivities"."ID_Rsv") LEFT OUTER JOIN "dbo"."Personnel" "Personnel" ON "RsvActivities"."ID_Personnel"="Personnel"."ID_Personnel") INNER JOIN "dbo"."Customers" "Customers" ON "RsvActivities"."ID_Customer"="Customers"."ID_Customer"
 WHERE  "RsvActivities"."BillState"&lt;0
 ORDER BY "Customers"."CustomerName",Start</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
